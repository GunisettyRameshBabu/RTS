@(Html.Kendo().DatePicker()
                .Name("StartDate")
                .Value("#=StartDate#") 
                .HtmlAttributes(new { style = "width: 60%", title = "Select Start Date", @required = "required" })
               .Format("dd MMM yyyy")
)

